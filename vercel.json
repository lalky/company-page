{
    "version": 2,
    "builds": [
        {
            "src": "api/sendMail.js",
            "use": "@vercel/node"
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/send-email",
            "dest": "api/sendMail.js"
        },
        {
            "src": "/(.*)",
            "dest": "/public/index.html"
        }
    ]
}
